   "source": [
    "from sklearn import datasets\n",
    "ds = datasets.make_classification()\n",
    "\n",
    "X = ds[0][:,[5,7]]\n",
    "y = ds[1]\n",
    "\n",
    "from sklearn import svm\n",
    "c = svm.SVC()\n",
    "\n",
    "c.fit(X,y)\n",
    "\n",
    "from plot import plot_decision_regions\n",
    "%matplotlib inline\n",
    "\n",
    "plot_decision_regions(X,y,c)"
   ]
